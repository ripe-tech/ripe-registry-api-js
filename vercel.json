{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        { "src": "/", "dest": "/docs/index.html" },
        { "src": "/(.*)", "dest": "/docs/$1" }
    ]
}
